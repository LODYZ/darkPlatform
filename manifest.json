{
	"name": "Dark platform",
	"description": "Un'estensione che rende platform un po' diverso",
	"version": "1.0.1",
	"manifest_version": 3,
	"icons": {
	"48": "/assets/icons/48.png",
	"128": "/assets/icons/128.png"
	},
	"permissions":[
		"tabs",
		"activeTab",
		"https://*/*",
		"scripting",
		"storage"
	],
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
	  "48": "/assets/icons/48.png",
	  "128": "/assets/icons/128.png"
	  }
	},
	"web_accessible_resources": [
		{
		  "resources": [ "customCSS.css", "/aceCss/*"  ],
		  "matches": ["*://*.google.com/*", "*://*/*" ]
		}
	],
	"content_scripts": [
		{
			"matches": ["*://*.google.com/*","*://*/*/4ws/main_designer*"],
			"js": ["inject.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	  }
}
